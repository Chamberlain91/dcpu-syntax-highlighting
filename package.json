{
    "name": "dcpu-syntax-highlight",
    "displayName": "DCPU-16 Language Support",
    "description": "Support for DCPU Assembly 1.7 & DCPUB",
    "version": "0.2.2",
    "publisher": "chamberlain",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Chamberlain91/dcpu-syntax-highlighting.git"
    },
    "icon": "icon.png",
    "engines": {
        "vscode": "^1.16.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "dcpub",
                "aliases": [
                    "DCPUB"
                ],
                "extensions": [
                    "b"
                ],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "dasm",
                "aliases": [
                    "DCPU Assembly",
                    "dasm"
                ],
                "extensions": [
                    "dasm"
                ],
                "configuration": "./dasm-language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "dcpub",
                "scopeName": "source.blang",
                "path": "./syntaxes/dcpub.tmLanguage.json"
            },
            {
                "language": "dasm",
                "scopeName": "source.dasm",
                "path": "./syntaxes/dasm.tmLanguage.json"
            }
        ]
    }
}